{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/app.py"
        }
    ],
    "installCommand": "pip install -r requirements.txt",
    "buildCommand": "echo 'No build command needed'",
    "outputDirectory": "."
}
